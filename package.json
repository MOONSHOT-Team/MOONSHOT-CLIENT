{
  "name": "moonshot-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --cache .",
    "preview": "vite preview",
    "postinstall": "husky install",
    "format": "prettier --cache --write ."
